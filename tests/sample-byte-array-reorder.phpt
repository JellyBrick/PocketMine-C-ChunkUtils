--TEST--
Tests byte-array re-ordering with some sample data.
--FILE--
<?php

use pocketmine\level\format\io\ChunkUtils;

$sample = str_repeat("\x00\x01\x02\x03\x04\x05\x06\x07\x08\x09\x0a\x0b\x0c\x0d\x0e\x0f", 256);

//Re-order 1, single rotation
$out = ChunkUtils::reorderByteArray($sample);
var_dump(bin2hex($out));

//Re-order 2, should produce same data as the original.
$final = ChunkUtils::reorderByteArray($out);
var_dump(bin2hex($final));
--EXPECT--
string(8192) ""
string(8192) ""