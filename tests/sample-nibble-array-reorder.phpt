--TEST--
Tests nibble-array re-ordering with some sample data.
--FILE--
<?php

use pocketmine\level\format\io\ChunkUtils;

$sample = str_repeat("\x01\x23\x45\x67\x89\xab\xcd\xef", 256);

//Re-order 1, single rotation
$out = ChunkUtils::reorderNibbleArray($sample);
var_dump(bin2hex($out)); //little-endian

//Re-order 2, should produce same data as the original.
$final = ChunkUtils::reorderNibbleArray($out);
var_dump(bin2hex($final));

//common_value micro optimization carried over from the PHP version is useless here, but including these should not produce any different behaviour
//Re-order 1, single rotation
$out = ChunkUtils::reorderNibbleArray($sample, "\xff");
var_dump(bin2hex($out)); //little-endian

//Re-order 2, should produce same data as the original.
$final = ChunkUtils::reorderNibbleArray($out, "\xff");
var_dump(bin2hex($final));
--EXPECT--
string(4096) ""
string(4096) ""
string(4096) ""
string(4096) ""